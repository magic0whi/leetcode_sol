{
  "version": 9,
  "configurePresets": [
    {
      "name": "default",
      "binaryDir": "${sourceDir}/build",
      "displayName": "Default Config",
      "description": "Default build using Ninja generator",
      "generator": "Ninja"
    },
    {
      "name": "debug",
      "inherits": "default",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "pgotrain",
      "inherits": "default",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "PGOTRAIN"
      }
    },
    {
      "name": "pgobuild",
      "inherits": "default",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "PGOBUILD"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "configurePreset": "debug"
    },
    {
      "name": "pgotrain",
      "configurePreset": "pgotrain"
    },
    {
      "name": "pgobuild",
      "configurePreset": "pgobuild"
    }
  ],
  "testPresets": [
    {
      "name": "pgotrain",
      "configurePreset": "pgotrain",
      "filter": {
        "include": {
          "name": "train"
        }
      }
    },
    {
      "name": "pgobuild",
      "configurePreset": "pgobuild",
      "filter": {
        "include": {
          "name": "run"
        }
      }
    }
  ],
  "workflowPresets": [
    {
      "name": "debug",
      "steps": [
        {
          "type": "configure",
          "name": "debug"
        },
        {
          "type": "build",
          "name": "debug"
        }
      ]
    },
    {
      "name": "pgotrain",
      "steps": [
        {
          "type": "configure",
          "name": "pgotrain"
        },
        {
          "type": "build",
          "name": "pgotrain"
        },
        {
          "type": "test",
          "name": "pgotrain"
        }
      ]
    },
    {
      "name": "pgobuild",
      "steps": [
        {
          "type": "configure",
          "name": "pgobuild"
        },
        {
          "type": "build",
          "name": "pgobuild"
        },
        {
          "type": "test",
          "name": "pgobuild"
        }
      ]
    }
  ]
}
